{
  "name": "cube-db",
  "productName": "CubeDB",
  "version": "1.2.0",
  "description": "The definitive speedcubing app",
  "homepage": "https://github.com/isaacvr/cubedb-svelte",
  "repository": {
    "type": "git",
    "url": "https://github.com/isaacvr/cubedb-svelte.git"
  },
  "private": true,
  "author": {
    "name": "Isaac Vega Rodriguez",
    "email": "isaacvega1996@gmail.com"
  },
  "keywords": [
    "cube",
    "rubik",
    "speedsolving",
    "speedcubing",
    "wca",
    "svelte",
    "electron",
    "nodejs",
    "typescript",
    "desktop",
    "windows"
  ],
  "main": "src/index.js",
  "scripts": {
    "start": "concurrently \"yarn dev\" \"electron-forge start -- --serve\"",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build": "vite build",
    "dev": "vite",
    "preview": "vite preview",
    "builder": "electron-builder build"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "electronRebuildConfig": {
        "forceABI": 113
      },
      "makers": [
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "win32"
          ]
        },
        {
          "name": "@electron-forge/maker-squirrel",
          "platforms": [
            "win32"
          ],
          "config": {
            "outputDirectory": "./release",
            "noMsi": true
          }
        }
      ]
    }
  },
  "dependencies": {
    "@electron/remote": "2.0.8",
    "@xstate/svelte": "^2.1.0",
    "ably": "^1.2.45",
    "archiver": "5.3.1",
    "async-lz-string": "^1.1.0",
    "axios": "^1.3.4",
    "chart.js": "4.0.1",
    "chartjs-plugin-zoom": "2.0.1",
    "electron-reload": "1.5.0",
    "electron-updater": "^5.3.0",
    "eruda": "^3.0.1",
    "express": "4.18.2",
    "js-confetti": "^0.11.0",
    "jszip": "3.10.1",
    "modern-palette": "^0.2.4",
    "moment": "2.29.4",
    "nedb": "1.8.0",
    "node-machine-id": "^1.1.12",
    "object-hash": "^3.0.0",
    "qrcode": "^1.5.3",
    "socket.io-client": "^4.7.2",
    "svelte-chartjs": "3.1.2",
    "svelte-material-icons": "3.0.5",
    "svelte-routing": "2.5.0",
    "three": "0.120.1",
    "unzipper": "^0.10.11",
    "xstate": "^4.38.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.50",
    "@electron-forge/maker-deb": "6.0.0-beta.50",
    "@electron-forge/maker-rpm": "6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "6.0.0-beta.50",
    "@electron-forge/maker-zip": "6.0.0-beta.50",
    "@sveltejs/vite-plugin-svelte": "1.1.0",
    "@tsconfig/svelte": "3.0.0",
    "@types/archiver": "^5.3.1",
    "@types/axios": "^0.14.0",
    "@types/nedb": "^1.8.12",
    "@types/object-hash": "^3.0.4",
    "@types/qrcode": "^1.5.2",
    "@types/web-bluetooth": "^0.0.17",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "autoprefixer": "10.4.16",
    "concurrently": "5.1.0",
    "electron": "27.0.0",
    "electron-builder": "24.6.4",
    "esbuild": "0.19.4",
    "postcss": "8.4.31",
    "postcss-load-config": "4.0.1",
    "svelte": "4.2.1",
    "svelte-check": "3.5.2",
    "svelte-preprocess": "5.0.4",
    "tailwindcss": "3.3.3",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "vite": "4.4.11"
  }
}
