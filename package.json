{
  "name": "cube-db",
  "productName": "CubeDB",
  "version": "1.0.0",
  "description": "The definitive speedcubing app",
  "homepage": "https://github.com/isaacvr/cube-db",
  "private": true,
  "author": {
    "name": "Isaac Vega Rodriguez",
    "email": "isaacvega1996@gmail.com"
  },
  "keywords": [
    "cube",
    "rubik",
    "speedsolving",
    "speedcubing",
    "wca",
    "svelte",
    "electron",
    "nodejs",
    "typescript",
    "desktop",
    "windows"
  ],
  "main": "src/index.js",
  "build": {
    "files": [
      "./dist/**/*"
    ],
    "asar": true
  },
  "scripts": {
    "start": "concurrently \"yarn dev\" \"electron-forge start -- --serve\"",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build": "vite build",
    "dev": "vite",
    "preview": "vite preview"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "electronRebuildConfig": {
        "forceABI": 113
      },
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "noMsi": false,
            "setupMsi": "CubeDB",
            "name": "CubeDB"
          }
        }
      ]
    }
  },
  "dependencies": {
    "@electron/remote": "2.0.8",
    "archiver": "5.3.1",
    "axios": "^1.3.4",
    "chart.js": "4.0.1",
    "chartjs-plugin-zoom": "2.0.0",
    "electron-reload": "1.5.0",
    "electron-squirrel-startup": "1.0.0",
    "express": "4.18.2",
    "jszip": "3.10.1",
    "moment": "2.29.4",
    "nedb": "1.8.0",
    "socket.io-client": "^4.6.0",
    "svelte-chartjs": "3.1.2",
    "svelte-material-icons": "2.0.4",
    "svelte-routing": "1.6.0",
    "three": "0.120.1",
    "unzipper": "^0.10.11"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.50",
    "@electron-forge/maker-deb": "6.0.0-beta.50",
    "@electron-forge/maker-rpm": "6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "6.0.0-beta.50",
    "@electron-forge/maker-zip": "6.0.0-beta.50",
    "@sveltejs/vite-plugin-svelte": "1.1.0",
    "@tsconfig/svelte": "3.0.0",
    "@types/archiver": "^5.3.1",
    "@types/axios": "^0.14.0",
    "@types/nedb": "^1.8.12",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "autoprefixer": "10.4.13",
    "concurrently": "5.1.0",
    "electron": "23.0.0",
    "electron-builder": "23.1.0",
    "esbuild": "0.16.14",
    "postcss": "8.4.18",
    "postcss-load-config": "4.0.1",
    "svelte": "3.55.1",
    "svelte-check": "2.9.2",
    "svelte-preprocess": "4.10.7",
    "tailwindcss": "3.2.1",
    "tslib": "2.4.0",
    "typescript": "4.8.4",
    "vite": "3.2.2"
  }
}
